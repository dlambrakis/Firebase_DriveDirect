{
      "$schema": "https://json.schemastore.org/tsconfig",
      "extends": "./base.json",
      "compilerOptions": {
        "jsx": "react-jsx",
        "lib": [
          "ES2022",
          "DOM",
          "DOM.Iterable"
        ],
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
      }
    }
