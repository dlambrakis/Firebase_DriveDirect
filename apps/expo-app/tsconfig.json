{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@directdrive/supabase-client": [
        "../../packages/supabase-client"
      ],
      "@directdrive/ui": [
        "../../packages/ui"
      ],
      "@directdrive/theme": [
        "../../packages/theme"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
